{
  "name": "my-todos",
  "version": "0.1.0",
  "type": "module",
  "description": "A TypeScript React Todo App",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"node src/server/server.js\"",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose --watchAll=false",
    "test:watch": "react-scripts test --verbose",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "keywords": [
    "react",
    "typescript",
    "todo"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.47.11",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.14",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.6",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "idb": "^8.0.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "schema-utils": "^4.0.1",
    "typescript": "^4.9.5",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^6.5.1",
    "fake-indexeddb": "^6.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "homepage": "https://eladmoshe.github.io/my-todos"
}
